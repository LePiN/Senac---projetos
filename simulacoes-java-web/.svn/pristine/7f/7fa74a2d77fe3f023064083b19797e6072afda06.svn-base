<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml"
	  xmlns:h="http://java.sun.com/jsf/html">
	<h:head>
		<title>Hello</title>
	</h:head>
	<h:body>
		<h:form>
			<h:commandButton value="Chamar hello()" 
							 action="#{helloBean.hello()}"/>
							 
			<h:commandButton value="Chamar teste()" 
							 action="#{testeBean.testar()}"/>				 
		</h:form>
					 						 	
		<h:outputText value="Mensagem do Hello: #{helloBean.mensagem}"/>
		<h:outputText value="Mensagem do Teste: #{testeBean.mensagem}"/>
	</h:body>	
</html>