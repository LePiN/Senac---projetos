CREATE SCHEMA 'exemplosbd' ;

CREATE TABLE MONTADORA(
	ID INT PRIMARY KEY AUTO_INCREMENT,
    NOME VARCHAR(255),
    PAIS VARCHAR(255)
);

CREATE TABLE CARRO(
  ID INT NOT NULL AUTO_INCREMENT,
  ID_MONTADORA INT NOT NULL,
  ANO INT NULL,
  MODELO VARCHAR(255) NOT NULL,
  VALOR DOUBLE NULL,
  PRIMARY KEY (ID),
  CONSTRAINT FK_MONTADORA
    FOREIGN KEY (ID_MONTADORA)
    REFERENCES MONTADORA (ID)
);
